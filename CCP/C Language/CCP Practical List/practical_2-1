 #include<stdio.h>
 #include<conio.h>

 void main()
 {
	  long p=80000;
	  long m,w,l,lm,lw,ilm,ilw;

	  clrscr();

	  m=(p*52)/100;
	  w=p-m;
	  l=(p*48)/100;
	  lm=(p*35)/100;
	  lw=l-lm;
	  ilm=m-lm;
	  ilw=w-lw;

	  printf("Total Population: %ld",p);
	  printf("\nNumber of literate: %ld",l);
	  printf("\nNumber of Men: %ld",m);
	  printf("\nNumber of literate men: %ld",lm);
	  printf("\nNumber of illiterate men: %ld",ilm);
	  printf("\nNumber of Women: %ld",w);
	  printf("\nNumber of literate women: %ld",lw);
	  printf("\nNumber of illiterate women: %ld",ilw);

	  getch();

 }
